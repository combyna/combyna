{
    "name": "combyna/combyna",
    "description": "Combyna - build things.",
    "autoload": {
        "psr-4": {
            "Combyna\\": ["php/src/Combyna/", "php/dist/Combyna/"]
        }
    },
    "require": {
        "symfony/event-dispatcher": "^2.8",
        "symfony/http-foundation": "^2.8",
        "symfony/yaml": "^2.8",
        "combyna/php-peg": "^2.0",
        "symfony/dependency-injection": "^2.8",
        "symfony/config": "^2.8",
        "symfony/routing": "^2.8",
        "symfony/translation": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "elliotchance/concise": "^2.1"
    },
    "scripts": {
        "build:expression-parser": "peg ./php/src/Combyna/Component/ExpressionLanguage/PegExpressionParser.php.inc ./php/dist/Combyna/Component/ExpressionLanguage/PegExpressionParser.php",
        "server": "php ./php/src/server.php",
        "simple-server": "php -S localhost:9000 example/simple/app.php"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Phillimore",
            "email": "dan@ovms.co"
        }
    ]
}
