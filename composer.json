{
    "name": "combyna/combyna",
    "description": "Combyna - build things.",
    "autoload": {
        "psr-4": {
            "Combyna\\": ["php/src/Combyna/"]
        }
    },
    "require": {
        "symfony/event-dispatcher": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/yaml": "^3.4",
        "combyna/php-peg": "^2.0",
        "symfony/dependency-injection": "^3.4",
        "symfony/config": "^3.4",
        "symfony/routing": "^3.4",
        "symfony/translation": "^3.4"
    },
    "require-dev": {
        "combyna/gui-plugin": "dev-symfony3",
        "phpunit/phpunit": "^7.5"
    },
    "scripts": {
        "server": "php ./php/src/server.php",
        "simple-server": "php -S localhost:9000 example/app.php"
    },
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Phillimore",
            "email": "dan@ovms.co"
        }
    ]
}
