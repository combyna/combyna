{
    "name": "combyna/combyna",
    "description": "Combyna - build things.",
    "autoload": {
        "psr-4": {
            "Combyna\\": ["php/src/Combyna/", "php/dist/Combyna/"]
        }
    },
    "require": {
        "symfony/event-dispatcher": "^3.1",
        "symfony/http-foundation": "^3.1",
        "hnesk/php-peg": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "elliotchance/concise": "^2.1"
    },
    "scripts": {
        "build:expression-parser": "php vendor/bin/cli.php ./php/dist/Combyna/ExpressionLanguage/PegExpressionParser.php.inc > ./php/dist/Combyna/ExpressionLanguage/PegExpressionParser.php",
        "server": "php ./php/src/server.php"
    },
