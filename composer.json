{
    "name": "combyna/combyna",
    "description": "Combyna - build things.",
    "autoload": {
        "psr-4": {
            "Combyna\\": ["php/src/Combyna/", "php/dist/Combyna/"]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/combyna/gui-plugin"
        }
    ],
    "require": {
        "symfony/event-dispatcher": "~2.8.0",
        "symfony/http-foundation": "~2.8.0",
        "symfony/yaml": "~2.8.0",
        "combyna/php-peg": "^2.0",
        "symfony/dependency-injection": "~2.8.0",
        "symfony/config": "~2.8.0",
        "symfony/routing": "~2.8.0",
        "symfony/translation": "~2.8.0"
    },
    "require-dev": {
        "combyna/gui-plugin": "dev-master",
        "phpunit/phpunit": "^4.6",
        "elliotchance/concise": "^2.1"
    },
    "scripts": {
        "server": "php ./php/src/server.php",
        "simple-server": "php -S localhost:9000 example/app.php"
    },
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Phillimore",
            "email": "dan@ovms.co"
        }
    ]
}
